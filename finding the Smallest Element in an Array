import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int min=Integer.MAX_VALUE;
		int a[]=new int[n];
		for(int i=0;i<n;i++){
		    a[i]=sc.nextInt();
		    if(a[i]<min){
		        min=a[i];
		    }
		}
		System.out.println("Smallest value is + " + min);
	}
}
